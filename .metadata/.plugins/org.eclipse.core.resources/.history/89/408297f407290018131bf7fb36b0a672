<mapper namespace="xyz.marsj.o2o.mapper.AreaMapper">
	<select id="queryShopCategory" resultType="xyz.marsj.o2o.entity.ProductCategory">
		SELECT 
		area_id,area_name,priority 
		FROM 
		tb_area 
		ORDER BY priority DESC
	</select>
</mapper>